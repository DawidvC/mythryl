	    fun adjust_origin (origin: g2d::Point, parent: Null_Or(Subwindow_Data))
		=
		case parent
		    #
		    NULL  =>    origin;
		    #
		    THE p =>    case p
				    #
				    SUBWINDOW_DATA (pm: Subwindow_Info)
					=>
					adjust_origin (origin + *pm.upperleft, pm.parent);
