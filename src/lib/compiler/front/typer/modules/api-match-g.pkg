																if_debugging_say ("check_named_type/tdt::SUM_TYPE/AAA name_per_api = " + name_per_api + "   src/lib/compiler/front/typer/modules/api-match-g.pkg");
					   type_per_api'
					       =
					       tdt::NAMED_TYPE
						 {
