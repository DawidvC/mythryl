	#
	fun unwrap_typecon_entry (mld::TYPE_ENTRY x) =>   x;
	    unwrap_typecon_entry _                  =>   bug "unwrap_typecon_entry";
        end;

        # Given a list of symbols created by
        #     src/lib/compiler/front/basics/map/symbol.pkg
        # return "a, b, c" or such:
