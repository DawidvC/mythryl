	fun by f x y
            =
            f y x;

	null_fix = INFIX (0, 0);
	inf_fix  = INFIX (1000000, 100000);

	fun stronger_l ( INFIX (_, m),
                         INFIX (n, _)
                       )
                =>
                m >= n;

	    stronger_l _
                =>
                FALSE;			#  should not matter 
        end;

	fun stronger_r ( INFIX (_, m),
                         INFIX (n, _)
                       )
                =>
                n > m;

	    stronger_r _
	        =>
                TRUE;			#  should not matter 
        end;

	fun prpos ( pp:  pp::Prettyprinter,
		   source:  sci::Sourcecode_Info,
                   charpos: Int
                 )
